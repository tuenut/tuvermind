upstream django {
    server unix:///tmp/tuvermind/uwsgi.sock;
    }

server {
    listen 80;
    server_name _;
    charset utf-8;
    client_max_body_size 75M;
    location /static {
        alias /var/www/tuvermind/static/;
        }
    location /media {
        alias /var/www/tuvermind/media/;
        }
    location / {
        uwsgi_pass django;
        include /etc/nginx/uwsgi_params;
        }
    }

