upstream django {
    server unix:///home/tuenut/PycharmProjects/tuvermind/uwsgi_django.sock;
    }

server {
    listen 80;
    server_name localhost;
    charset utf-8;
    client_max_body_size 75M;
    location /static {
        alias /home/tuenut/PycharmProjects/tuvermind/static/;
        }
    location / {
        uwsgi_pass django;
        include /etc/nginx/uwsgi_params;
        }
    }

